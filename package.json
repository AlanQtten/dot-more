{
  "name": "dot-more",
  "displayName": "Dot More",
  "description": "vscode plugin,easy to generate code",
  "version": "1.1.0",
  "publisher": "AlanQtten",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.7.0"
  },
  "packageManager": "pnpm@8.0.0+",
  "keywords": [
    "webstorm",
    "log",
    "console",
    "autocomplete"
  ],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlanQtten/dot-more.git"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "main": "./out/extension.cjs",
  "contributes": {
    "configuration": {
      "title": "dotMore",
      "properties": {
        "dotMore.disableReactExtends": {
          "type": "boolean",
          "default": false,
          "description": "whether to disable react grammar extend"
        },
        "dotMore.disableAutoImport": {
          "type": "boolean",
          "default": true,
          "description": "whether to disable auto import api, such as `useState`"
        },
        "dotMore.maxLengthOfLogmPrefixMessage": {
          "type": "number",
          "description": "max length of logm prefix message"
        },
        "dotMore.alwaysCloneLogResult": {
          "type": "boolean",
          "default": false,
          "description": "whether to use `structuredClone` for log result"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "vitest --run",
    "compile": "vite build --mode production",
    "debug": "vite build --watch",
    "test": "vitest --coverage --bail 1",
    "build": "pnpm compile && vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-v8": "^1.1.2",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  },
  "type": "module"
}
